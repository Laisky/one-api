# Development environment configuration for Default template
FAST_REFRESH=true
GENERATE_SOURCEMAP=true
# This will be overridden by the version from package.json script if needed
REACT_APP_VERSION=dev
# Disable eslint plugin for faster development builds
DISABLE_ESLINT_PLUGIN=true
# Development server configuration - Default template uses port 3001
PORT=3001
# Fix webpack dev server allowedHosts issue
DANGEROUSLY_DISABLE_HOST_CHECK=true
# Alternative: specify allowed hosts explicitly
WDS_SOCKET_HOST=localhost
